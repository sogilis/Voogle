            "get": {
                "description": "Get list of all videos",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "video"
                ],
                "summary": "Get list of all videos",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Sort attribute",
                        "name": "attribute",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Sort order",
                        "name": "order",
                        "in": "path",
                        "required": true
