                    {
                        "type": "string",
                        "description": "Page number",
                        "name": "page",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Video per page",
                        "name": "limit",
                        "in": "path",
                        "required": true
